{{#instance}}
  <div class="row-fluid">
    <div class="span12">
      <object-loader {path}="instance.cycle">
        <a class="main-title {{class.category}} oneline"
           href="/workflows/{{loadedObject.workflow.id}}">
        {{title}}
      </a>
      </object-loader>
    </div>
  </div>

  <div class="row-fluid">
    <div class="span12">
      <h6>Description</h6>
      <div class="rtf">
        {{{firstnonempty description '<span class="empty-message">None</span>'}}}
      </div>
    </div>
  </div>

  <div class="row-fluid">
    <div class="span12">
      <div class="code">
        Code: {{slug}}
      </div>
    </div>
  </div>
{{/instance}}
