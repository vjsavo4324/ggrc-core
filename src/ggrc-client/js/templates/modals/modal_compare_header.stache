<span>{{modal_title}}</span>
<div class="legend">
  <span class="removed"></span> - removed
  <span class="added"></span> - added
</div>

{{#if displayDescriptions}}
  <div class="columns-description">
    <div class="left-column">
      <div class="column-content">
        <div>
          {{leftRevisionData.description}}
        </div>
        <person-data {person}="{leftRevisionData.author}"></person-data>
        <div>
          {{dateTime leftRevisionData.date}}
        </div>
      </div>
    </div>
    <div class="column-content">
      <div>
        {{rightRevisionData.description}}
      </div>
      <person-data {person}="{rightRevisionData.author}"></person-data>
      <div>
        {{dateTime rightRevisionData.date}}
      </div>
    </div>
  </div>
{{/if}}
