{{#if isChangeableExternally}}
  <a
    class="questionnaire-link {{linkClass}}"
    href="{{url}}"
    target="_blank">
      {{#if showIcon}}
        <i class="fa fa-external-link questionnaire-link__icon {{#is(iconPosition, "right")}}questionnaire-link__icon_position_right{{/is}}"></i>
      {{/if}}
      <content></content>
  </a>
{{/if}}
