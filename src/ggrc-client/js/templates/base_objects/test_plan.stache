{{#instance}}
  {{#if_helpers '\n #if' test_plan '\n or #if' isRevision}}
    <div class="row-fluid wrap-row">
      <div class="span12">
        <h6>Assessment Procedure</h6>
        <div class="rtf-block">
          <read-more
            {text}="{test_plan}"
            {handle-markdown}="{instance.class.isChangeableExternally}">
          </read-more>
        </div>
      </div>
    </div>
  {{/if_helpers}}
{{/instance}}
